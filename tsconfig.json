{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "compilerOptions": {
        "paths": {
            "@/*": ["./src/*"]
        },
        "strict":true,
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.d.ts",
        "src/**/*.tsx",
        "src/**/*.vue",
        "./**/*.ts"
    ],
}
